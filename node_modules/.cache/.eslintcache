D(response.user.id));\n\t\t\tdispatch(setName(response.user.username));\n\t\t\tCookies.set('token', response.jwt);\n\t\t\thistory.push(\"/\");\n\t\t})\n\t\t.catch((error) => setDisplayError('Mauvais identifiant / password'));\n  \t}\n\n\treturn (\n\t\t<div className='Login'>\n\t\t  <form onSubmit={handleSubmit(onSubmit)} className=\"Register\">\n\t\t    <input name=\"identifier\" type=\"email\" placeholder=\"email\" ref={register({ required: true })} />\n\t\t    <input name=\"password\" type=\"password\" placeholder=\"password\" ref={register({ required: true })} />\n\t  \t    <input type=\"submit\" />\n\t  \t    <p>{displayError}</p>\n\t  \t    <Link to=\"/register\">\n          \t  <p>Register</p>\n        \t</Link>\n\t      </form>\n\n\t    </div>\n\t);\n};\n\nexport default Login;\n","/Users/mathieujoly/Documents/Next_THP/Semaine07/J31/the-K/the-k/src/components/Form/Form.jsx",[],"/Users/mathieujoly/Documents/Next_THP/Semaine07/J31/the-K/the-k/src/actions/index.js",[],"/Users/mathieujoly/Documents/Next_THP/Semaine07/J31/the-K/the-k/src/reducers/hasID.js",[],"/Users/mathieujoly/Documents/Next_THP/Semaine07/J31/the-K/the-k/src/pages/Profile/Profile.jsx",["92","93","94","95"],"import React, { useState, useEffect } from 'react';\nimport Cookies from 'js-cookie';\nimport { useSelector } from 'react-redux';\nimport { useForm } from 'react-hook-form';\nimport { useHistory } from 'react-router-dom';\nimport './Profile.scss';\n\nconst Profile = ({ match }) => {\n\tconst id = match.url.match(/\\d+/g);\n\tconst [username, setUsername] = useState('');\n\tconst [description, setDescription] = useState('');\n\tconst [displayError, setDisplayError] = useState('');\n\n\tconst loadProfile = () => {\n  \t\tfetch(`http://localhost:1337/users/${id}`, {\n  \t\t  method: 'get',\n  \t\t  headers: {\n    \t\t'Authorization': `Bearer ${Cookies.get('token')}`,\n    \t\t'Content-Type': 'application/json'\n  \t\t  }\n\t\t})\n\t\t.then((response) => response.json())\n\t\t.then((response) =>